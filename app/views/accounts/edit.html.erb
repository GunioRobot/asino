<%= render :partial => 'layouts/sidebar' %>
<div class="column right">

<h1>Konto bearbeiten</h1>

<% form_for(@account) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title, 'Name des Kontos' %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :feed, '@accounts = Account.all' %><br />
    <%= f.text_field :feed %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

</div>

<div class="clr"> </div>